# ZSH
ZSH_THEME="gentoo"
DISABLE_AUTO_UPDATE="true"
plugins=(brew git github ruby rbenv cp heroku osx autojump tmux tmuxinator zsh-syntax-highlighting)
# hey! zsh-syntax-highlighting must be the last declared plugin.

# SHELL
PROFILE="$HOME/.zshrc"

# LOCALE
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# GLOBAL
export EDITOR="vim"
export GIT_EDITOR="vim"


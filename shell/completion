# Vault
_vault_complete() {
  local word completions
  word="$1"
  completions="$(vault --cmplt "${word}")"
  reply=( "${(ps:\n:)completions}" )
}
compctl -f -K _vault_complete vault

# Dir complete
function c() { cd ~/code/$@; };
compctl -/ -W ~/code c
function h() { cd ~/$@; };
compctl -/ -W ~/ h


if [[ "$OSTYPE" == "cygwin" ]]; then

if [ -z "$USERID" ]; then
    echo "Enviroment variable USERID doesnt exist. See ~/.sensitive"
fi

function cc { cd /cygdrive/c/ ; }
function c10 { cd /cygdrive/c/EMPRO/SIARH/10g ; }
function coc { cd /cygdrive/c/EMPRO/Projetos/SIARH/Ocorrencias ; }

function f6 { ifbld60 "$1" $USERID & }
function r6 { rwbld60 "$1" $USERID & }
function f10 { frmbld "$1" $USERID & }
function r10 { rwbuilder "$1" $USERID & }
function p { plsqldev userid=$USERID nosplash sessionmode=multi "$1" & }

alias f=f10
alias r=r10

function sps { echo exit | sqlplus -S $USERID @"$1" ; }

# an ugly hack to not load Oracle Translation Hub at ZSH statup
alias hub=''

fi

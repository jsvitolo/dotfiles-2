# Git Config Template

[hub]
        protocol = https
[user]
        name = AUTHORNAME
        email = AUTHOREMAIL
[alias]
        co = checkout
        ci = commit
        st = status
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        default = simple

